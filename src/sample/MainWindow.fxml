<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.shape.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.Line?>

<Pane fx:id="myPane" prefHeight="501.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <children>
      <TabPane fx:id="myTabPane" prefHeight="500.0" prefWidth="900.0" tabClosingPolicy="UNAVAILABLE">
        <tabs>
          <Tab fx:id="encryptTab" text="ADD SEQUENCE">
               <content>
              <AnchorPane fx:id="anchorPaneEncryptMode" prefHeight="501.0" prefWidth="900.0">
                     <children>

                         <Button fx:id="addSequencesFromFileButton" layoutX="26.0" layoutY="315.0" mnemonicParsing="false" onAction="#openFile" prefHeight="25.0" prefWidth="157.0" text="Add Sequences From File" />
                         <TextArea fx:id="sequenceTextArea" layoutX="21.0" layoutY="62.0" prefHeight="205.0" prefWidth="258.0" promptText="" wrapText="true" />
                         <Button fx:id="addButton" layoutX="311.0" layoutY="361.0" mnemonicParsing="false" onAction="#addSequence" prefWidth="279.0" text="Add Sequence" />
                         <Label layoutX="20.0" layoutY="23.0" prefHeight="21.0" prefWidth="259.0" text="Sequence from file or input:" />
                         <Label layoutX="389.0" layoutY="25.0" prefHeight="17.0" prefWidth="162.0" text="Sequence type:" />
                         <ListView fx:id="sequencesListView" layoutX="389.0" layoutY="62.0" prefHeight="143.0" prefWidth="162.0" />
                         <CheckBox fx:id="fromFileCheckBox" layoutX="26.0" layoutY="280.0" mnemonicParsing="false" text="From File" />
                     </children>
                  </AnchorPane>
               </content>
          </Tab>
            <Tab fx:id="decryptTab" text="MULTI-ALIGNMENT">
               <content>
                  <AnchorPane fx:id="anchorPaneDecryptMode" prefHeight="501.0" prefWidth="900.0">
                     <children>
                        <ListView fx:id="scoringTypesListView" layoutX="288.0" layoutY="56.0" prefHeight="216.0" prefWidth="162.0" />
                        <Label layoutX="288.0" layoutY="39.0" prefHeight="17.0" prefWidth="162.0" text="Score type:" />
                        <Label layoutX="34.0" layoutY="39.0" prefHeight="17.0" prefWidth="162.0" text="Profile:" />
                        <ListView fx:id="profilesListView" layoutX="34.0" layoutY="56.0" prefHeight="206.0" prefWidth="162.0" />
                        <Button fx:id="calculateButton" layoutX="281.0" layoutY="345.0" mnemonicParsing="false" onAction="#calculateMultiAlignment" prefWidth="279.0" text="Mult-Alignment Calculate" />
                     </children>
                  </AnchorPane>
               </content>
            </Tab>
        </tabs>
      </TabPane>
   </children>
</Pane>
