<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<Pane fx:id="myPane" prefHeight="501.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/8"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
    <children>
        <TabPane fx:id="myTabPane" prefHeight="500.0" prefWidth="900.0" tabClosingPolicy="UNAVAILABLE">
            <tabs>
                <Tab fx:id="encryptTab" text="ADD SEQUENCE">
                    <content>
                        <AnchorPane fx:id="anchorPaneEncryptMode" prefHeight="501.0" prefWidth="900.0">
                            <children>

                                <Button fx:id="addSequencesFromFileButton" layoutX="21.0" layoutY="373.0"
                                        mnemonicParsing="false" onAction="#openFile" prefHeight="30.0" prefWidth="191.0"
                                        text="Add Sequences From File">
                                    <font>
                                        <Font size="14.0"/>
                                    </font>
                                </Button>
                                <TextArea fx:id="sequenceTextArea" layoutX="21.0" layoutY="62.0" prefHeight="254.0"
                                          prefWidth="292.0" promptText="" wrapText="true"/>
                                <Button fx:id="addButton" layoutX="461.0" layoutY="310.0" mnemonicParsing="false"
                                        onAction="#addSequence" prefWidth="279.0" text="Add Sequence">
                                    <font>
                                        <Font size="18.0"/>
                                    </font>
                                </Button>
                                <Label layoutX="20.0" layoutY="23.0" prefHeight="21.0" prefWidth="259.0"
                                       text="Sequence from file or input:">
                                    <font>
                                        <Font name="Arial" size="14.0"/>
                                    </font>
                                </Label>
                                <Label layoutX="502.0" layoutY="25.0" prefHeight="17.0" prefWidth="162.0"
                                       text="Choose sequence type:">
                                    <font>
                                        <Font name="Arial" size="14.0"/>
                                    </font>
                                </Label>
                                <ListView fx:id="sequencesListView" layoutX="502.0" layoutY="62.0" prefHeight="143.0"
                                          prefWidth="162.0"/>
                                <CheckBox fx:id="fromFileCheckBox" layoutX="21.0" layoutY="335.0"
                                          mnemonicParsing="false" text="From File">
                                    <font>
                                        <Font name="Arial" size="14.0"/>
                                    </font>
                                </CheckBox>
                                <Label layoutX="461.0" layoutY="373.0" prefHeight="26.0" prefWidth="280.0" text="">
                                    <font>
                                        <Font size="14.0"/>
                                    </font>
                                </Label>
                                <Label fx:id="addInfoText" layoutX="461.0" layoutY="373.0" prefHeight="26.0" prefWidth="280.0" text="" />
                                <Button fx:id="clearButton" onAction="#clear" layoutX="324.0" layoutY="62.0"
                                        mnemonicParsing="false" prefHeight="30.0" prefWidth="75.0" text="Clear">
                                    <font>
                                        <Font size="14.0"/>
                                    </font>
                                </Button>
                            </children>
                        </AnchorPane>
                    </content>
                </Tab>
                <Tab fx:id="decryptTab" text="MULTI-ALIGNMENT">
                    <content>
                        <AnchorPane fx:id="anchorPaneDecryptMode" prefHeight="501.0" prefWidth="900.0">
                            <children>
                                <ListView fx:id="scoringTypesListView" layoutX="245.0" layoutY="48.0" prefHeight="237.0"
                                          prefWidth="160.0"/>
                                <Label layoutX="245.0" layoutY="23.0" prefHeight="17.0" prefWidth="162.0"
                                       text="Choose Score type:">
                                    <font>
                                        <Font size="14.0"/>
                                    </font>
                                </Label>
                                <Label layoutX="26.0" layoutY="23.0" prefHeight="17.0" prefWidth="162.0"
                                       text="Choose Profile:">
                                    <font>
                                        <Font name="Arial" size="14.0"/>
                                    </font>
                                </Label>
                                <ListView fx:id="profilesListView" layoutX="26.0" layoutY="50.0" prefHeight="232.0"
                                          prefWidth="154.0"/>
                                <Button fx:id="calculateButton" layoutX="68.0" layoutY="355.0" mnemonicParsing="false"
                                        onAction="#calculateMultiAlignment" prefWidth="279.0"
                                        text="Mult-Alignment Calculate">
                                    <font>
                                        <Font size="18.0"/>
                                    </font>
                                </Button>
                                <Label layoutX="466.0" layoutY="13.0" prefHeight="21.0" prefWidth="259.0"
                                       text="Result:">
                                    <font>
                                        <Font name="Arial" size="14.0"/>
                                    </font>
                                </Label>
                                <TextArea fx:id="resultTextArea" layoutX="466.0" layoutY="38.0" prefHeight="395.0"
                                          prefWidth="404.0" promptText="" wrapText="true"/>
                            </children>
                        </AnchorPane>
                    </content>
                </Tab>
            </tabs>
        </TabPane>
    </children>
</Pane>
